{
  "name": "@darwin/schema",
  "version": "0.0.1",
  "author": "Andrew Brown",
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.module.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "microbundle",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@vitest/ui": "^0.29.3",
    "microbundle": "^0.15.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.2",
    "vitest": "^0.29.3"
  }
}